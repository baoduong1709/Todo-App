<% var tasksTodo = [];%>
<% var tasksInprogress = [];%>
<% var tasksCompleted = []; %>
<% allTasks.forEach(function(task) {%>
<%     if (task.status_id === 1) {%>
<%        tasksTodo.push(task);%>
<%     } else if (task.status_id === 2) {%>
<%         tasksInprogress.push(task);%>
<%     } else if (task.status_id === 3) {%>
<%         tasksCompleted.push(task);%>
<%     }%>
<% });%>